{
  "notebook": {
    "expand": false,
    "show_side_bar": false,
    "external_editor": "tmux display-popup -x {left} -w {width} -h 70% -E nvim",
    "show_top_bar": false,
    "show_cell_borders": false,
    "clipboard": "internal",
    "multiplexer_passthrough": true
  },
  "kernel_name": "python3",
  "edit_mode": "vi",
  "autocomplete": true,
  "autosuggest": true,
  "autoinspect": true,
  "autoformat": false,
  "record_cell_timing": true,
  "show_file_icons": true,
  "show_shadows": false,
  "show_status_bar": false,
  "enable_language_servers": true,
  "key_bindings": {
    "euporie.core.app.BaseApp": {
      "next-tab": "c-right",
      "previous-tab": "c-left",
      "next-completion": "c-j",
      "previous-completion": "c-k",
      "accept-suggestion": "tab"
    },
    "euporie.notebook.app.NotebookApp": {
      "show-command-palette": "c-p"
    },
    "euporie.notebook.tabs.notebook.Notebook": {
      "enter-cell-edit-mode": "i",
      "add-cell-above": "O",
      "add-cell-below": "o",
      "interrupt-kernel": [],
      "scroll-up": "<scroll-up>",
      "scroll-down": "<scroll-down>"
    }
  }
}
