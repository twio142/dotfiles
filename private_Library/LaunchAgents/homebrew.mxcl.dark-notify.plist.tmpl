<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.homebrew.dark-notify</string>
		<key>ProgramArguments</key>
		<array>
      <string>/opt/homebrew/bin/dark-notify</string>
			<string>-c</string>
      <string>/usr/local/bin/appearanceHandler</string>
		</array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>{{ env "HOME" }}/Library/Logs/dark-notify.log</string>
    <key>StandardOutPath</key>
    <string>{{ env "HOME" }}/Library/Logs/dark-notify.log</string>
</dict>
</plist>

